{
  "scripts": {
    "build": "webpack --mode=production --config=webpack.prod.js",
    "test": "jest",
    "format": "prettier --write \"src/**/*.{ts,js}\"",
    "lint-es": "prettier --check \"src/**/*.{ts,js}\"",
    "lint": "eslint . --ext .js --ext .ts",
    "lint-es-fix": "eslint . --ext .js --ext .ts --fix",
    "build:dev": "webpack --mode=development --config=webpack.prod.js",
    "build:prod": "webpack --mode=production --node-env=production --config=webpack.prod.js",
    "watch": "webpack --watch",
    "serve": "webpack serve --mode=development --config=webpack.dev.js --open"
  },
  "dependencies": {
    "bootstrap": "^5.2.2",
    "bootstrap-icons": "^1.9.1",
    "canvas-confetti": "^1.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@types/bootstrap": "^5.2.6",
    "@types/jest": "^29.1.2",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.1.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-webpack-plugin": "^3.2.0",
    "fork-ts-checker-notifier-webpack-plugin": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "jest": "^29.2.0",
    "jest-environment-jsdom": "^29.2.0",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.17",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.2.1",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.54.9",
    "sass-loader": "^13.1.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.(ts)": [
      "npm run --silent lint"
    ]
  },
  "version": "1.0.0",
  "description": "My webpack project",
  "name": "my-webpack-project"
}
